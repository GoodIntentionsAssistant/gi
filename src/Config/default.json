{
  "name": "Good Intentions",
  "paths": {
    "root": "/../..",
    "app": "/../../app",
    "temp": "/../../app/Temp",
    "logs": "/../../app/Temp/Log",
    "package": "/../../app/Temp/Package",
    "system": "/../../src",
    "data": "/../../data",
    "skills": {
      "app": "/../../app/Skill",
      "sys": "/../../src/Skill"
    }
  },
  "app": {
    "loop_speed": 500
  },
  "database": {
    "driver": "jfs"
  },
  "clients": {
    "test": {
      "secret": "NrCgyKqvyB"
    }
  },
  "collections": {
    "admin": {
      "classifier": "strict"
    },
    "cancel": {
      "classifier": "strict"
    },
    "strict": {
      "classifier": "strict"
    },
    "default": {
      "classifier": "classify"
    },
    "fallback": {
      "classifier": "fallback"
    }
  },
  "auth": {
    "strict": false
  },
  "logging": {
    "enabled": true
  },
  "response": {
    "min_reply_time": 500,
    "letter_speed": 20,
