    "timeout": 5000
  },
  "expects": {
    "expire": 60
  },
  "packages": {
    "url": "https://raw.githubusercontent.com/GoodIntentionsAssistant/gi-packages/master/gi-packages.json"
  },
  "server": {
    "enabled": true,
    "port": 3000
  }
}